<!DOCTYPE html>

<html>
<head>
    <title></title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="1.css" />
    <style>
        * {
            box-sizing: border-box;
        }

        html, body {
            height: 100%;
            margin: 0px;
            padding: 0px;
        }

        .contain {
            width: 100%;
            height: 100%;
        }

        .header {
            height: 94px;
            position: absoulte;
            top: 0px;
            width: 100%;
            background: #f6d654 ;
        }

        .content {
            height: 100%;
            position: relative;
            background: white;
            max-height: 1300px;
        }

            .content .data {
                padding: 10px;
            }

            .content .footer {
                height: 93px;
                position: absolute;
                bottom: 0px;
                left: 0px;
                width: 100%;
                background: #f6d654 url('../img/footer1.jpg');
            }

        #name {
            font-family: Comic Sans MS;
            font-size: 40px;
            color: #914220;
            text-align: center;
            font-weight: 800;
        }

        }
    </style>
</head>
<body>
    <div class="contain">
        <div class="content">
            <div class="header">
                <p id="name">Embroidery.NET</p>
            </div>
            <div class="data">
                @RenderBody();
            
                </div>
            <div class="footer"></div>
        </div>
    </div>

</body>
</html>
